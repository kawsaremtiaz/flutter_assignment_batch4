void main() {
  List<int> numbers = [85, 92, 78, 65, 88, 72];

  int totalNumbers = 0;
  int totalSub = numbers.length;

  for (int number in numbers) {
    totalNumbers += number; 
  }

  double avarageNumbers = totalNumbers/totalSub;
  int roundAvarage = avarageNumbers.round();
  
  results(){
   if(avarageNumbers >= 70){
    print("Passed");
  }
  else{
    print("Failed");
  }
  }
  print("Student avarage numbers grade: $avarageNumbers");
  print("Student Rounded average numbers: $roundAvarage");
  results();
}
